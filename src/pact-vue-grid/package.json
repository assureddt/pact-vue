{
	"name": "@assureddt/pact-vue-grid",
	"version": "0.0.48",
	"description": "Vue 3 grid system",
	"repository": {
		"type": "git",
		"url": "https://github.com/assureddt/pact-vue/"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "6.1.1",
		"@fortawesome/vue-fontawesome": "3.0.0-3",
		"core-js": "3.22.3",
		"vue": "3.2.33"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "5.21.0",
		"@typescript-eslint/parser": "5.21.0",
		"@vue/cli-plugin-babel": "5.0.4",
		"@vue/cli-plugin-eslint": "5.0.4",
		"@vue/cli-plugin-typescript": "5.0.4",
		"@vue/cli-service": "5.0.4",
		"@vue/compiler-sfc": "3.2.33",
		"@vue/eslint-config-typescript": "10.0.0",
		"eslint": "8.14.0",
		"eslint-plugin-vue": "^8.7.1",
		"typescript": "4.6.3",
		"webpack": "5.72.0"
	},
	"main": "dist/pact-vue-grid.common.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build-debug": "vue-cli-service build --dest dist --mode development --target lib src/index.ts && tsc --emitDeclarationOnly",
		"build": "vue-cli-service build --dest dist --mode production --target lib src/index.ts && tsc --emitDeclarationOnly",
		"lint": "vue-cli-service lint"
	},
	"keywords": [
		"vue",
		"bootstrap",
		"grid"
	],
	"author": "assureddt",
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended",
			"@vue/typescript/recommended"
		],
		"parserOptions": {
			"ecmaVersion": 2020
		},
		"rules": {
			"@typescript-eslint/ban-ts-ignore": "off"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
